{
    "manifest_version": 3,
    "name": "CsgoskinsExtension",
    "version": "1.0.1",
    "content_scripts": [{
        "matches": ["https://csgo-skins.com/*"],
        "js": [
            "content.js",
            "utils/currency.js",
            "utils/html_elems.js",
            "skins/page_data.js",
            "skins/case_skins.js"
        ]
    }],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": ["tabs", "webNavigation"],
    "background": {
        "service_worker": "background.js"
    }
}